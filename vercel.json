{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "functions": {
    "api/**/*.{ts,js}": {
      "maxDuration": 10
    },
    "api/videos/scrape-url.{ts,js}": {
      "maxDuration": 60
    },
    "api/videos/import-from-url.{ts,js}": {
      "maxDuration": 60
    }
  },
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}